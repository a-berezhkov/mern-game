    var modalInstance = $uibModal.open({
      animation: true,
      templateUrl: '/pa/pa.confirm.html?05f9d40d98425d0a',
      controller: 'paConfirmCtrl',
      resolve: {
        message: function(){
          return 'Ð’Ñ‹ ÑƒÐ²ÐµÑ€ÐµÐ½Ñ‹? ÐžÑ‚ÐºÐ°Ð·Ð°Ð²ÑˆÐ¸ÑÑŒ Ð¾Ñ‚ ÑƒÑ‡Ð°ÑÑ‚Ð¸Ñ Ð² Ð²ÐµÐ±Ð¸Ð½Ð°Ñ€Ðµ, Ð’Ñ‹ Ð±ÑƒÐ´ÐµÑ‚Ðµ Ð¸Ð·ÑƒÑ‡Ð°Ñ‚ÑŒ Ð¿Ð»Ð°Ñ‚Ñ„Ð¾Ñ€Ð¼Ñƒ Ð¡ÐÐœÐžÐ¡Ð¢ÐžÐ¯Ð¢Ð•Ð›Ð¬ÐÐž Ð¿Ð¾ Ð²ÐµÐ±Ð¸Ð½Ð°Ñ€Ð°Ð¼ Ð² Ð·Ð°Ð¿Ð¸ÑÐ¸?'
